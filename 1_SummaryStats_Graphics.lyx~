\begin_layout Enumerate
Match the following histograms to the appropriate boxplot.
\begin_inset Newline newline
\end_inset


\begin_inset Box Frameless
position "t"
hor_pos "c"
has_inner_box 1
inner_pos "t"
use_parbox 0
use_makebox 0
width "100col%"
special "none"
height "1in"
height_special "totalheight"
status open

\begin_layout Plain Layout
\begin_inset ERT
status open

\begin_layout Plain Layout

<<echo=FALSE, fig.height=4>>=
\end_layout

\begin_layout Plain Layout

n <- 20000 
\end_layout

\begin_layout Plain Layout

m1 <- rnorm(n, mean=20, sd=2.5) 
\end_layout

\begin_layout Plain Layout

m2 <- rnorm(n, mean=20, sd=10) 
\end_layout

\begin_layout Plain Layout

m3 <- rgamma(n, shape=4, scale=15/4) 
\end_layout

\begin_layout Plain Layout

m4 <- c(rnorm(n/2, mean=0, sd=6), rnorm(n/2, mean=30, sd=6))
\end_layout

\begin_layout Plain Layout

d1 <- data.frame(obs=c(m1,m2,m3,m4), Group=rep(c('A','B','C','D'), each=n))
 
\end_layout

\begin_layout Plain Layout

d2 <- data.frame(obs=c(m2,m1,m4,m3), Group=rep(c('1','2','3','4'), each=n))
 
\end_layout

\begin_layout Plain Layout

library(ggplot2)
\end_layout

\begin_layout Plain Layout

p <- ggplot(d1, aes(obs)) +    
\end_layout

\begin_layout Plain Layout

	geom_histogram(aes(y=..density..), binwidth=1) +   
\end_layout

\begin_layout Plain Layout

	facet_wrap(~Group, scales='free') + xlab('Observed Value')
\end_layout

\begin_layout Plain Layout

print(p)
\end_layout

\begin_layout Plain Layout

@
\end_layout

\end_inset


\end_layout

\end_inset


