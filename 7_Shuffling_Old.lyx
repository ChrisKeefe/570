\end_layout

\begin_layout Plain Layout

# correction and just calculate the test statistic as we've described 
\end_layout

\begin_layout Plain Layout

chisq.test(                                                # do a Chi-sq
 test
\end_layout

\begin_layout Plain Layout

  tally(outcome ~ treatment, data=Mites, format='count'),  # on this table
\end_layout

\begin_layout Plain Layout

